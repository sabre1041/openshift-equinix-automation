apiVersion: v1
baseDomain: {{ base_domain }}
compute: 
- name: worker
  replicas: 0 
controlPlane: 
  name: master
  replicas: 1 
metadata:
  name: {{ cluster_name }}
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14 
    hostPrefix: 23 
  networkType: OVNKubernetes 
  machineNetwork:
  - cidr: {{ node_gateway }}/{{ node_netmask }}
  serviceNetwork: 
  - 172.30.0.0/16
platform:
  none: {} 
fips: false 
pullSecret: '{{ pull_secret|string }}'
sshKey: '{{ ssh_public_key }}'
